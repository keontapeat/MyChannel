<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>TMDB_API_KEY</key>
	<string>$(TMDB_API_KEY)</string>
	<key>AI_API_KEY</key>
	<string>$(AI_API_KEY)</string>

	<!-- URL Types for Google Sign-In -->
	<key>CFBundleURLTypes</key>
	<array>
		<dict>
			<key>CFBundleTypeRole</key>
			<string>Editor</string>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>com.googleusercontent.apps.124515086975-mu4mm52mrsp5k7n760g1q57llasrte5t</string>
			</array>
		</dict>
	</array>

	<!-- Privacy Usage Descriptions -->
	<key>NSPhotoLibraryUsageDescription</key>
	<string>MyChannel needs access to your photo library to let you pick videos and images for upload.</string>
	<key>NSPhotoLibraryAddUsageDescription</key>
	<string>MyChannel saves exported videos or edited media to your library when you choose to.</string>
	<key>NSCameraUsageDescription</key>
	<string>MyChannel uses the camera so you can record videos and create content.</string>
	<key>NSMicrophoneUsageDescription</key>
	<string>MyChannel uses the microphone to capture audio while recording videos or going live.</string>
</dict>
</plist>